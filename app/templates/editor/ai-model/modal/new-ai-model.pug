extends /templates/editor/modal/new-model-modal

block modal-body-content
  form.form
    .form-group
      label.control-label(for="name", data-i18n="general.name") Name
      input#name.form-control(name="name", type="text")

    .form-group
      label.control-label(for="family", data-i18n="general.family") Family
      input#family.form-control(name="family", type="text")

block modal-footer
  .modal-footer
    button.btn(data-dismiss="modal", data-i18n="common.cancel") Cancel
    button#save-new-ai-model-link.btn.btn-primary.new-model-submit(data-i18n="common.create") Create
